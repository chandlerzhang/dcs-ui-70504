{
  "entry": "src/index.js",
  "theme":"./theme.js",
  "publicPath":"./",
  "externals":{
      "react":"window.React",
      "react-dom":"window.ReactDOM"
  },
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": true }]
      ]
    }
  }
}
